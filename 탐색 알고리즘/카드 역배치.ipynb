   "source": [
    "a=list(range(21))\n",
    "for _ in range(10):\n",
    "    s, e = map(int, input().split())\n",
    "    for i in range((e-s+1)//2):\n",
    "        a[s+i], a[e-i]=a[e-i], a[s+i]\n",
    "a.pop(0)\n",
    "for x in a:\n",
    "    print(x, end=' ')"
